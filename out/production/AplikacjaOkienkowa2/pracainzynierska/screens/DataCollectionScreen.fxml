<Pane fx:id="progressBar" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" style="-fx-background-size: 1,1,1,1;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pracainzynierska.controllers.DataCollectionController">
   <children>
      <Button fx:id="startButton" disable="true" layoutX="348.0" layoutY="235.0" mnemonicParsing="false" onAction="#startReading" prefHeight="59.0" prefWidth="94.0" text="START" textFill="#11c76c">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ComboBox fx:id="comboBox1" layoutX="14.0" layoutY="14.0" onAction="#detectPort" prefHeight="50.0" prefWidth="177.0" />
