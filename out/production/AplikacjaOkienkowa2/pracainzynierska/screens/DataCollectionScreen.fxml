      <Button fx:id="stopButton" disable="true" layoutX="348.0" layoutY="303.0" mnemonicParsing="false" onAction="#stopReading" prefHeight="59.0" prefWidth="94.0" text="STOP" textFill="#d51313">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="connectButton" layoutX="215.0" layoutY="14.0" mnemonicParsing="false" onAction="#connect" prefHeight="50.0" prefWidth="114.0" text="Połącz" />
