      <Button fx:id="viewDataButton" layoutX="89.0" layoutY="195.0" mnemonicParsing="false" onAction="#viewData" text="Podgląd danych" />
      <TextArea fx:id="textArea1" layoutX="38.0" layoutY="233.0" prefHeight="210.0" prefWidth="233.0" />
      <Button fx:id="visalisationButton" layoutX="531.0" layoutY="470.0" mnemonicParsing="false" onAction="#visualise" prefHeight="50.0" prefWidth="164.0" text="Wizualizacja danych" />
